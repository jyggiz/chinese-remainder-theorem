{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "models/*": ["src/models/*"],
      "hooks/*": ["src/hooks/*"],
      "helpers/*": ["src/helpers/*"]
    },
  },
  "include": [
    "src/components/index.tsx"
  ]
}